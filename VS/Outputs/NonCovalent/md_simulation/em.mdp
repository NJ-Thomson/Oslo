; Energy minimization
integrator          = steep
emtol               = 1000.0
emstep              = 0.01
nsteps              = 50000

; Neighbor searching
nstlist             = 10
cutoff-scheme       = Verlet
pbc                 = xyz

; Electrostatics
coulombtype         = PME
rcoulomb            = 1.0

; VdW
vdwtype             = Cut-off
rvdw                = 1.0

; Other
constraints         = none
